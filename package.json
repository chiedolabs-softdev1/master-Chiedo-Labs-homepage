{
  "name": "homepage.react",
  "version": "1.0.0",
  "engines": {
    "node": "10.15.0"
  },
  "description": "Chiedo-Labs-homepage",
  "main": "index.js",
  "repository": "https://github.com/chiedolabs-softdev1/master-Chiedo-Labs-homepage",
  "author": "Chiedo Labs",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "indentation": 2,
      "declaration-block-semicolon-newline-after": "always",
      "function-parentheses-space-inside": "never-single-line",
      "no-descending-specificity": null,
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "mixin",
            "include",
            "each",
            "function",
            "return",
            "if",
            "else"
          ]
        }
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "classnames": "^2.2.6",
    "css-modules": "^0.3.0",
    "dotenv": "^6.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "^7.0.2",
    "next-redux-wrapper": "^2.1.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "webpack": "^4.28.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "heroku-postbuild": "next build"
  },
  "devDependencies": {
    "@chiedolabs/eslint-config-nextjs": "latest",
    "@chiedolabs/eslint-config-react": "latest",
    "css-loader": "0.28.10",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0"
  }
}
